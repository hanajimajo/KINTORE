<h1>画像投稿フォーム</h1>
<%= form_with model: @training, local:true do |f| %>
  <h4>画像</h4>
  <%= f.attachment_field :image %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>時間×セット</h4>
  <%= f.text_field :time %>×<%= f.text_field :set %>
  <h4>説明</h4>
  <%= f.text_area :description %>
  <%= f.submit '投稿' %>
<% end %>