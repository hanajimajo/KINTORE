  <div class="search_form">
    <%= form_with url: search_path, local: true, method: :get do |f| %>
      <%= f.text_field :word,placeholder: "キーワード検索" %>
      <%#= f.select :range, options_for_select([['Training']]) %>
      <div class = "select"><%= f.select :search, options_for_select([["部分一致","partial_match"]]) %></div>
      <%= f.submit "検索", class: "btn btn-secondary btn-sm py-0" %>
    <% end %>
  </div>